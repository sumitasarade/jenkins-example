pipeline {
	agent any
	
	stages {
		stage {
			stage('Compile stage'){
				steps{
					withMaven(maven: 'MAVEN_HOME'){
						sh 'mvn clean compile'
					}
				}
			}
			
			stage('Testing Stage'){
				steps{
					withMaven(maven: 'MAVEN_HOME'){
						sh 'mvn test'
					}
				}
			}
			
			stage('Dummy Stage'){
				steps{
					echo 'Dummy Stage'
				}
			}
		}
	}
}
